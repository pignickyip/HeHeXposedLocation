<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
        <item name="android:colorPrimary">?android:attr/colorPrimary</item>
    </style>

    <style name="hehestyle" parent="Theme.AppCompat.Light">
        <item name="android:navigationBarColor">@color/hehecolorAccent</item>
        <item name="android:colorForeground">@color/hehecolorAccent</item>
        <item name="android:statusBarColor">?android:attr/navigationBarColor</item>
    </style>

</resources>